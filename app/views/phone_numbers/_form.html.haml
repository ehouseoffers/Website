= form_for @phone_number do |f|
  = render :partial => 'shared/user_notice', :locals => {:model_obj => @phone_number}

  .field
    = f.label :user_id
    = f.text_field :user_id
  .field
    = f.label :number
    = f.text_field :number
  .field
    = f.label :label
    = f.text_field :label
  .field
    = f.label :primary
    = f.check_box :primary
  .actions
    = f.submit 'Save'
